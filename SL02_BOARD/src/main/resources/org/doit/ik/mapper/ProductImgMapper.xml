<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="org.doit.ik.mapper.ProductImgMapper">

	<select id="getImg" resultType="org.doit.ik.domain.ProductImgDTO">
		SELECT *
		FROM productimg
		WHERE productid = #{id}
	</select>

</mapper>
